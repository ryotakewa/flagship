<div class="field">
  <label>ID</label>
  <%= @category.id %>
</div>
<div class="field">
  <label>カテゴリ</label>
  <%= @category.category.name if @category.category.present? %>
</div>
<div class="field">
  <label>名称</label>
  <%= @category.name %>
</div>

<div class="vs">
  <%= link_to '一覧', admin_categories_path, { class: "btn btn-default" } %>
  <%= link_to '編集', edit_admin_category_path(@category), { class: "btn btn-default" } %>
</div>
